<script type="text/javascript">
//<![CDATA[
jQuery(document).ready(function($){
	if(listdeal.length > 0){
		for(i=0;i<listdeal.length;i++)
		{
			var arr = listdeal[i].split("&&||&&"); 
			var data = new Date(arr[1]);
			CountDown(data, arr[0]);
		}	
	}
});
//]]>
</script>	
